---
permalink: /feed.xml
excludeFromCollections: true
layout: false
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Will Simpson's Notes</title>
    <link>https://willsimpson.netlify.app</link>
    <description>A simple, lightweight, and flexible note-taking template for Eleventy.</description>
    <language>en-us</language>
    <lastBuildDate>{{ now | rssDate }}</lastBuildDate>
    <atom:link href="https://willsimpson.netlify.app/feed.xml" rel="self" type="application/rss+xml" />
    {%- for post in collections.allPosts %}
    <item>
      <title>{{ post.data.title | escapeXml }}</title>
      <link>https://willsimpson.netlify.app/n/{{ post.fileSlug }}</link>
      <guid isPermaLink="true">https://willsimpson.netlify.app/n/{{ post.fileSlug }}</guid>
      <pubDate>{{ post.data.cdate | rssDate }}</pubDate>
      <description><![CDATA[{{ post.content | cleanHtml }}]]></description>
    </item>
    {%- endfor %}
  </channel>
</rss>
